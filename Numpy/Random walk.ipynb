   "source": [
    "indices=np.random.choice([-1,1],size=1000)\n",
    "sum1=np.cumsum(indices)\n",
    "max_distance=np.abs(sum1).max()\n",
    "max_index=np.abs(sum1).argmax()\n",
    "print(max_distance,max_index)"
   ]
